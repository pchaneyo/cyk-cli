{
   "name": "@cyklang/cli",
   "version": "0.1.2",
   "description": "cyklang CLI",
   "main": "build/index.js",
   "scripts": {
      "build": "tsc",
      "global": "npm run build && npm i -g",
      "local": "sudo npm uninstall -g && sudo npm install -g && cyk",
      "npm:format": "npx prettier-package-json --write package.json",
      "npm:upgrade": "npx npm-check-updates -iu",
      "refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
      "dev": "nodemon -e ts --ignore build --exec \"npm run global\" ",
      "test": "cyk test test/t*.xml"
   },
   "keywords": [],
   "author": "",
   "license": "ISC",
   "dependencies": {
      "@cyklang/core": "^0.3.4",
      "commander": "^9.4.0",
      "dotenv": "^16.0.1",
      "form-data": "^4.0.0",
      "loglevel": "^1.8.0",
      "mime-types": "^2.1.35",
      "pako": "^2.0.4",
      "xml": "^1.0.1"
   },
   "devDependencies": {
      "@types/mime-types": "^2.1.1",
      "@types/pako": "^2.0.0",
      "@types/sax": "^1.2.4",
      "@types/xml": "^1.0.8",
      "nodemon": "^2.0.16",
      "ts-node": "^10.7.0",
      "typescript": "4.5.2"
   },
   "bin": {
      "cyk": "build/index.js"
   }
}
